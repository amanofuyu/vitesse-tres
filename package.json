		"tweakpane": "^4.0.4",
		"vue": "^3.5.3",
		"vue-router": "^4.4.3"
	},
	"devDependencies": {
		"@antfu/eslint-config": "^3.4.0",
		"@biomejs/biome": "1.9.4",
		"@tweakpane/core": "^2.0.4",
		"@types/node": "^22.5.4",
		"@types/three": "^0.169.0",
		"@vitejs/plugin-vue": "^5.1.3",
		"@vue-macros/volar": "^0.28.1",
		"@vue/test-utils": "^2.4.6",
		"autoprefixer": "^10.4.20",
		"daisyui": "^4.12.14",
		"jsdom": "^25.0.0",
		"lint-staged": "^15.2.10",
