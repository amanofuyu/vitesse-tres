		"unplugin-vue-router": "^0.10.7",
		"vite": "^5.4.3",
		"vite-plugin-glsl": "^1.3.0",
		"vitest": "^2.0.5",
		"vue-tsc": "^2.1.6"
	},
	"simple-git-hooks": {
		"pre-commit": "pnpm lint-staged"
